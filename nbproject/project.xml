    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>eXist-1.0</name>
            <properties/>
            <folders>
                <source-folder>
                    <label>src</label>
                    <type>java</type>
                    <location>src</location>
                </source-folder>
                <source-folder>
                    <label>wrapper</label>
                    <type>java</type>
                    <location>tools/wrapper/src</location>
                </source-folder>
                <source-folder>
                    <label>samples</label>
                    <type>java</type>
                    <location>samples</location>
                </source-folder>
                <source-folder>
                    <label>extensions\modules\src</label>
                    <type>java</type>
                    <location>extensions/modules/src</location>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>all</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="javadoc">
                    <target>javadocs</target>
                </action>
                <action name="run">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>run-nb</target>
                </action>
                <action name="run.single">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>run-nb.single</target>
                    <context>
                        <property>classname</property>
                        <folder>src</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="test">
                    <target>test</target>
                </action>
                <action name="test.single">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>test-nb.single</target>
                    <context>
                        <property>classname</property>
                        <folder>src</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>all</target>
                </action>
                <action name="debug">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>debug-nb</target>
                </action>
                <action name="debug.single">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>debug-nb.single</target>
                    <context>
                        <property>classname</property>
                        <folder>src</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <!--action name="profile.single">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>profile-yourkit.single</target>
                    <context>
                        <property>classname</property>
                        <folder>src</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action-->
                <action name="profile">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>profile-yourkit</target>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>build/classes</location>
                <build-target>all</build-target>
            </export>
            <export>
                <type>folder</type>
                <location>extensions/modules/classes</location>
                <build-target>all</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>src</label>
                        <location>src</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>wrapper</label>
                        <location>tools/wrapper/src</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>samples</label>
                        <location>samples</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>extensions\modules\src</label>
                        <location>extensions/modules/src</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="clean"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="test"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="debug"/>
                    <action>
                        <script>nbproject/ide-file-targets.xml</script>
                        <label>Profile eXist-DB using "YourKit"</label>
                        <target>profile-yourkit</target>
                    </action>
                    <action>
                        <script>nbproject/ide-file-targets.xml</script>
                        <label>Profile eXist-DB using "NetBeans"</label>
                        <target>profile-nb</target>
                    </action>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
