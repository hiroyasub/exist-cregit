<?xml version="1.0" encoding="UTF-8"?>

<!-- 
	Search through a set of RDF references. This example uses the 
	XMLDBTransformer to process the query. The actual query will
	be constructed by the bibquery.xsl stylesheet and inserted below.
-->

<document xmlns:ci="http://apache.org/cocoon/include/1.0">

	<header>
    	<logo src="logo.jpg"/>
    	<title>Open Source XML Database</title>
    	<author email="meier@ifs.tu-darmstadt.de">Wolfgang M. Meier</author>
	</header>
       
    <!-- include sidebar -->
    <ci:include src="sidebar.xml"/>
   
  	<body>
  
    	<section title="Query results">
    	
    	<xdb:collection 
			xmlns:xdb="http://exist-db.org/transformer/1.0"
			uri="xmldb:exist:///db">
				<!-- stylesheet inserts query below: -->
				<create-query/>
		</xdb:collection>
		
		</section>
	</body>
</document>
